	<!-- SemanticMapPerception한 부분-->

	<owl:NamedIndividual rdf:about="&arbi;semanticMapPerception0">
		<rdf:type rdf:resource="&knowrob;SemanticMapPerception"/>
		<knowrob:startTime rdf:resource="&arbi;timepoint_1535974641"/>
